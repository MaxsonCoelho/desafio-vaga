{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@core/*": ["src/core/*"],
      "@assets/*": ["src/assets/*"],
      "@utils/*": ["src/utils/*"],
      "@navigators": ["src/navigators/index.tsx"],
      "@navigators/*": ["src/navigators/*"],
      "@locales": ["src/locales/index.ts"],
      "@locales/*": ["src/locales/*" ],
      "@commons/*": ["./commons/student-front-commons/src/*"],
      "@features/*": ["src/features/*"],
      "@implementations/*": ["src/implementations/*"],
    }
  },
}
